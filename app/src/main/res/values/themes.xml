<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TransparentDialogTheme" parent="Theme.MaterialComponents.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Theme.SUPMAP1" parent="android:Theme.Material.Light.NoActionBar" />

    <style name="Base.Theme.SplashScreenApp" parent="Theme.Material3.DayNight.NoActionBar" />

    <style name="Theme.App.SplashScreen" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/black</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/blanc</item>
        <item name="postSplashScreenTheme">@style/Base.Theme.SplashScreenApp</item>
    </style>

    <style name="Theme.SUPMAP" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:fontFamily">sans-serif</item>
        <item name="colorPrimary">@color/orange</item>
        <item name="colorPrimaryVariant">@color/orange</item>
        <item name="colorOnPrimary">@android:color/white</item>
        <item name="android:statusBarColor">@color/orange</item>
    </style>
</resources>